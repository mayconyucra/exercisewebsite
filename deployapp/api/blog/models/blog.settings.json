{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "portadablog": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "tituloblog": {
      "type": "string",
      "required": true
    },
    "fechablog": {
      "type": "date",
      "required": true
    },
    "descripcionblog": {
      "type": "text"
    }
  }
}
